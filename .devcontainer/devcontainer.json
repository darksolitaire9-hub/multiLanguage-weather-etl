{
  "name": "Multi-Language Weather ETL",
  "image": "ubuntu:22.04",
  "postCreateCommand": "bash .devcontainer/setup.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "REditorSupport.r",
        "julialang.language-julia",
        "ms-toolsai.jupyter",
        "github.vscode-pull-request-github"
      ]
    }
  },
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "label": "Airflow Web UI",
      "onAutoForward": "notify"
    }
  }
}
