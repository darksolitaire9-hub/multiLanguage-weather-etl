{
  "name": "Multi-Language Weather ETL",
  "image": "mcr.microsoft.com/devcontainers/python:3.12-jammy",
  "postCreateCommand": "bash .devcontainer/setup.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "REditorSupport.r",
        "julialang.language-julia",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "label": "Airflow Web UI",
      "onAutoForward": "notify"
    }
  }
}
